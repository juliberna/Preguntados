<div class="w3-container w3-content w3-center w3-padding-64" style="max-width:800px" id="band">
    <h2 class="w3-wide">Integrantes</h2>
    <table class="w3-table">
        <tr>
            <th>id</th>
            <th>nombre</th>
            <th>instrumento</th>
        </tr>
        {{#integrantes}}
            <tr>
                <td>{{id}}</td>
                <td>{{nombre}}</td>
                <td>{{instrumento}}</td>
            </tr>
        {{/integrantes}}
    </table>
    <div class="w3-center w3-margin w3-padding w3-sand">
        <h3>Sumate!</h3>
        <form action="/group/add" method="post">
            Nombre:<input type="text" name="nombre"/>
            Instrumento:<input type="text" name="instrumento"/>
            <button type="submit">Enviar</button>
        </form>
    </div>
</div>


